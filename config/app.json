{
    "name": "Starter app",
    "userInterface": {
        "pageTitle": "Cool application"
    },
    "paths": {
        "migrations": "base:database/migrations/",
        "models": "app:Models/",
        "repositories": "app:Libraries/Repositories/",
        "controllers": "app:Http/Controllers/",
        "views": "base:resources/views/",
        "assets": "base:public/",
        "routes": "app:Http/routes.php"
        "developer": "app:Http/developer_routes.php"
    },
    "namespaces": {
        "models": "App\\Models",
        "repositories": "App\\Libraries\\Repositories",
        "controllers": "App\\Http\\Controllers"
    },
    "inheritance": {
        "controller": "Scaffolder\\Controller\\AppBaseController",
        "model": "Illuminate\\Database\\Eloquent\\Model"
    },
    "routing": {
        "prefix": "scaffolder"
    },
    "api": {
        "domain": "cool.app.domain"
    }
}
